# Configurações para ambiente de teste
NODE_ENV=test
VUE_APP_API_BASE_URL=http://localhost:3000
VUE_APP_WS_URL=ws://localhost:3002
VUE_APP_TEST_MODE=true

# Configurações de banco de teste
DB_HOST=localhost
DB_PORT=5432
DB_NAME=surestake_test
DB_USER=test_user
DB_PASSWORD=test_password

# Configurações de cache para testes
CACHE_TTL=1000
CACHE_MAX_SIZE=100

# Configurações de rate limiting para testes
RATE_LIMIT_REQUESTS=1000
RATE_LIMIT_WINDOW=60000

# Configurações de WebSocket para testes
WS_HEARTBEAT_INTERVAL=30000
WS_RECONNECT_INTERVAL=5000
WS_MAX_RECONNECT_ATTEMPTS=5

# Configurações de logging para testes
LOG_LEVEL=error
LOG_TO_FILE=false

# Configurações de performance para testes
PERFORMANCE_MONITORING=false
METRICS_COLLECTION=false
