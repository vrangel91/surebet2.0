{
  "name": "surebets-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run copy-favicon",
    "build:watch": "vue-cli-service build --watch",
    "build:auto": "npm run build && pm2 restart surestake",
    "copy-favicon": "mkdir -p dist/img/icons && cp src/assets/img/icons/favicon.svg dist/img/icons/",
    "lint": "vue-cli-service lint",
    "build:prod": "npm run build && cd dist && npx serve -s . -p 8080",
    "serve:prod": "cd dist && npx serve -s . -p 8080",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --reporter=verbose src/**/*.test.js",
    "test:integration": "vitest run --reporter=verbose src/**/*.integration.test.js",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@vueuse/core": "^10.4.1",
    "axios": "^1.6.0",
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.5.0",
    "lucide-vue-next": "^0.542.0",
    "mitt": "^3.0.1",
    "vue": "^3.3.4",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.2.5",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "@testing-library/vue": "^8.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^1.0.4",
    "@vue/cli-plugin-pwa": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "^2.4.2",
    "cypress": "^13.6.0",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.14.1",
    "jsdom": "^23.0.1",
    "msw": "^2.0.8",
    "sass": "^1.70.0",
    "sass-loader": "^14.0.0",
    "vitest": "^1.0.4",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
